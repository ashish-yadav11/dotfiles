appearance.headingpos=right-justified
daemon.enable=no
daemon.log=no
format.inputdate=2
format.notifydate=%a, %b %d
format.notifytime=%R
format.outputdate=%x
format.dayheading=%B %e, %Y
general.autogc=yes
general.autosave=yes
general.confirmdelete=yes
general.confirmquit=no
general.firstdayofweek=monday
general.multipledays=yes
general.periodicsave=0
general.systemevents=yes
notification.command=o=$(calcurse -n); o=${o#*\[}; h=${o%%:*}; o=${o#*:}; m=${o%%\]*}; t=$(( h * 60 + m )); notify-send -t "$(( t * 60000 ))" Calcurse "Appointment in $t minute(s)\n${o#*\] }"
notification.notifyall=unflagged-only
notification.warning=300
